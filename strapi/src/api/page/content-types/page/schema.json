{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "string",
      "maxLength": 200,
      "required": true,
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$"
    },
    "seo": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": true
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "shared.rich-text",
        "shared.media"
      ]
    }
  }
}
